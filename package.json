{
  "name": "mediawiki-wikiforum",
  "version": "2.1.0",
  "description": "Provide a front-end structured discussion page with JavaScript.",
  "main": "index.js",
  "repository": "https://github.com/Wjghj-Project/Gadget-WikiForum.git",
  "author": "Dragon-Fish <824399619@qq.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn clear && webpack && MINIFY=1 webpack && yarn style",
    "dev": "yarn clear && webpack --watch -o ./dev/",
    "clear": "rm -rf ./dist ./**/dist ./dev",
    "lint": "eslint ./index.js ./module/*.js ./method/*.js",
    "prettier": "prettier --write ./index.js ./module/*.js ./method/*.js",
    "publish:stable": "npm publish --tag latest",
    "publish:canary": "npm run build && npm publish --tag canary && yarn clear",
    "style": "stylus ./theme/default.styl -o ./public/dist/WikiForum.theme.default.css"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "babel-loader": "^8.2.2",
    "stylus": "^0.54.8",
    "terser-webpack-plugin": "^5.1.1",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.3.0"
  },
  "dependencies": {}
}
